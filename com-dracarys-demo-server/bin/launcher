#!/bin/bash
/usr/local/jdk/bin/java -server -Xms2048m -Xmx2048m -Xss256k -XX:PermSize=128m -XX:MaxPermSize=384m -Dproject.home=/home/apps/current -Dproject.name=VenusService -Dproject.output=/home/apps/current/logs -Dignore.signals=1,2 -Dproject.pid.path=/home/apps/current/ -Dclassworlds.conf=/home/apps/current/bin/launcher.classpath -DmyParam=value1 -DmyParam2=value2 -classpath /home/apps/current/lib/plexus-classworlds-2.4.4-HEXNOVA.jar org.codehaus.classworlds.Launcher